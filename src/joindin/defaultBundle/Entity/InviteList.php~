<?php

namespace ji\defaultBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ji\defaultBundle\Entity\InviteList
 *
 * @ORM\Table(name="invite_list")
 * @ORM\Entity
 */
class InviteList
{
    /**
     * @var integer $id
     *
     * @ORM\Column(name="ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer $eid
     *
     * @ORM\Column(name="eid", type="integer", nullable=true)
     */
    private $eid;

    /**
     * @var integer $uid
     *
     * @ORM\Column(name="uid", type="integer", nullable=true)
     */
    private $uid;

    /**
     * @var string $accepted
     *
     * @ORM\Column(name="accepted", type="string", length=1, nullable=true)
     */
    private $accepted;

    /**
     * @var integer $dateAdded
     *
     * @ORM\Column(name="date_added", type="integer", nullable=true)
     */
    private $dateAdded;


}