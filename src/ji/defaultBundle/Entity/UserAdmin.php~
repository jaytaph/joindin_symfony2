<?php

namespace ji\defaultBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ji\defaultBundle\Entity\UserAdmin
 *
 * @ORM\Table(name="user_admin")
 * @ORM\Entity
 */
class UserAdmin
{
    /**
     * @var integer $id
     *
     * @ORM\Column(name="ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer $uid
     *
     * @ORM\Column(name="uid", type="integer", nullable=true)
     */
    private $uid;

    /**
     * @var integer $rid
     *
     * @ORM\Column(name="rid", type="integer", nullable=true)
     */
    private $rid;

    /**
     * @var string $rtype
     *
     * @ORM\Column(name="rtype", type="string", length=20, nullable=true)
     */
    private $rtype;

    /**
     * @var string $rcode
     *
     * @ORM\Column(name="rcode", type="string", length=40, nullable=true)
     */
    private $rcode;


}